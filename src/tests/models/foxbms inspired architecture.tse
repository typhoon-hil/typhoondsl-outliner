switch (BMS_State)
{
case 0: /* disabled during init and reset states */
case 6:
{
    break;
}
case 1: /* estimate OCV w/ Pre charge Voltage */
{
    for (i = 0; i < cell_count; ++i )
    {
        if (coulomb_eff_counter*execution_rate <= coulomb_eff_timer)
            {
            precharge_voltage[i] = cell_volt[i];
