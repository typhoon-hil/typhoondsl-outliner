        }
        energy = currents[i]*execution_rate/3600; /* convert current to amp hrs */
        SOC_Coulomb[i] -= energy/Capacity;
        if (SOC_Coulomb[i] < 0) /*min SOC */
        {
            SOC_Coulomb[i] = 0;
        }
    }
break;
}
case 4: /* charging */
{
