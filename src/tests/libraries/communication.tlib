                            number_of_inputs = mdl.get_property_value(mdl.prop(item_handle, "number_of_inputs"))

                            if endianess == 'Big Endian':
                                endianess = 'big_endian'
                            else:
                                endianess = 'little_endian'

                            if not conf:
                                """
                                This part of the code handles backward compatibility
                                """
