                        }
                        [ position = 8100, 8100 ]

                        component "core/Bus Join" "IQ_bus_join" {
                            inputs =" 4"
                        }
                        [ position = 8100, 8200 ]

                        component "core/Bus Join" "V_bus_join" {
                            inputs =" 4"
                        }
                        [ position = 8100, 8300 ]

                        component "core/Bus Join" "VQ_bus_join" {
                            inputs =" 4"
