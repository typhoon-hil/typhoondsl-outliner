                                            kind=api_const.ERROR_PROPERTY_VALUE_INVALID,
                                            context=mdl.prop(item_handle, "src_ip")
                                        )
                                        return

                            conf = mdl.get_property_value(mdl.prop(item_handle, "configuration"))
                            conf = literal_eval(conf)

                            val_error = check_dict(conf)
                            if val_error:
                                mdl.error(
